#!/bin/bash

sofa-project() {
    mvn org.apache.maven.plugins:maven-archetype-plugin:2.0-alpha-5:generate -DarchetypeCatalog=http://mvn.dev.alipay.net:8081/artifactory/content/repositories/Alipay-Releases/com/alipay/sofa/archetype/archetypes/1.0.0/archetypes-1.0.0.xml

}

sofa-build() {
  bash run.sh build
}

sofa-deploy() {
  bash run.sh deploy
}

sofa-run() {
  bash /home/admin/adccore-run/bin/jbossctl.sh start
}

ssh-alipay() {
	ssh mingming.xumm@shterm.alipay.net
}
alias stm=ssh-alipay

